    <decisions>
        <description>Checks to see if the portal contact returned a result and directs user to error screen if not.</description>
        <name>dec_Portal_Contact_Found</name>
        <label>Portal Contact Found?</label>
        <locationX>481</locationX>
        <locationY>313</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>out_Portal_Contact_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recLookup_Portal_Contact</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>asgmt_Portal_Contact</targetReference>
            </connector>
            <label>Portal Contact Found</label>
        </rules>
        <rules>
            <name>out_Portal_Contact_Not_Found</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>recLookup_Portal_Contact</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>asgmt_Portal_Contact_Error_Message</targetReference>
            </connector>
            <label>Portal Contact Not Found</label>
        </rules>
    </decisions>
