    <validationRules>
        <fullName>Actual_Draft_Remand_released_Not_Blank</fullName>
        <active>true</active>
        <description>You cannot enter an Actual Draft Released Party Date until the Calculate Draft Released Party Date is not populated</description>
        <errorConditionFormula>AND( 
ISNULL( Calculated_Draft_Remand_release_to_party__c ), 
NOT( ISNULL( Actual_Draft_Remand_released_to_party__c )) 
)</errorConditionFormula>
        <errorMessage>Calculated Draft Remand Released Date must be populated in order to enter Actual Draft Remand Released Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Actual_Final_Signature_Not_Blank</fullName>
        <active>true</active>
        <description>You cannot enter an Actual Final Signature Date until the Calculate Final Signature Date is not populated</description>
        <errorConditionFormula>AND( 
ISNULL( Calculated_Final_Signature__c ), 
NOT( ISNULL( Actual_Final_Signature__c )) 
)</errorConditionFormula>
        <errorMessage>Calculated Final Signature Date must be populated in order to enter Actual Final Signature Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Actual_Initiation_Signature_Not_Blank</fullName>
