    </validationRules>
    <validationRules>
        <fullName>Actual_Preliminary_Signature_Not_Blank</fullName>
        <active>true</active>
        <description>You cannot enter an Actual Preliminary Signature Date until the Calculate Preliminary Signature Date is not populated</description>
        <errorConditionFormula>AND( 
ISNULL( Calculated_Preliminary_Signature__c ), 
NOT( ISNULL( Actual_Preliminary_Signature__c )) 
)</errorConditionFormula>
        <errorMessage>Calculated Preliminary Signature Date must be populated in order to enter Actual Preliminary Signature Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Every_Litigation_must_have_a_Parent</fullName>
        <active>true</active>
        <description>Every Litigation must have either a Petition, Investigation, or Segment</description>
        <errorConditionFormula>AND( ISBLANK(Petition__c),
     ISBLANK(Segment__c),  
     ISBLANK(Investigation__c)
)</errorConditionFormula>
        <errorMessage>Every Litigation must have either a Petition, Investigation, or Segment. Please enter a  Petition, Investigation, or Segment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>International_Litigation_Final_Extension</fullName>
        <active>true</active>
        <errorConditionFormula>And (
$RecordType.DeveloperName = &quot;International_Litigation&quot;, 
  Final_Extension_of_days__c  &lt; 0
)</errorConditionFormula>
        <errorMessage>Final Extension cannot be less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>International_Litigation_Preli_Extension</fullName>
        <active>true</active>
        <errorConditionFormula>And (
$RecordType.DeveloperName = &quot;International_Litigation&quot;, 
   Prelim_Extension_of_days__c  &lt; 0
)</errorConditionFormula>
        <errorMessage>Preliminary Extension cannot be less than zero.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Remand_Final_Extension</fullName>
        <active>true</active>
        <errorConditionFormula>And (
$RecordType.DeveloperName = &quot;Remand&quot;, 
  Final_Extension_of_days__c  &lt; 0
)</errorConditionFormula>
        <errorMessage>Final Extension cannot be less than zero.</errorMessage>
    </validationRules>
