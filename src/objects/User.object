    <validationRules>
        <fullName>Prevent_Role_Hierarchy_Updates</fullName>
        <active>false</active>
        <description>Inactivated for Potential Future Use.  Prevents Role Hierarchy Updates for all users except System Administrators.</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot; &amp;&amp;
ISCHANGED(RH_Lev_6_Office__c) ||
ISCHANGED(RH_Lev_1_Top_Level__c) ||
ISCHANGED(RH_Lev_2_DAS_Level__c) ||
ISCHANGED(RH_Lev_3_Region_2__c) ||
ISCHANGED(RH_Lev_4_Region_1__c) ||
ISCHANGED(RH_Lev_5_Country__c) ||
ISCHANGED(RH_Lev_7_Individual__c) ||
ISCHANGED(ITA_Office__c)
)</errorConditionFormula>
        <errorMessage>Only System Administrators may update Role Hierarchies.</errorMessage>
    </validationRules>
