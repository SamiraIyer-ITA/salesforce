    <validationRules>
        <fullName>V01CheckNumberMustBeNumeric</fullName>
        <active>true</active>
        <description>For check payments, the check number must be numeric.</description>
        <errorConditionFormula>AND(
  $RecordType.DeveloperName = &apos;Check_Payment&apos;,
  NOT(ISNUMBER( Check_Number__c ))
)</errorConditionFormula>
        <errorDisplayField>Check_Number__c</errorDisplayField>
        <errorMessage>The check number must be numeric.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>V02RefundExplanationRequired</fullName>
        <active>true</active>
        <description>If the Refund Reason is &quot;Other&quot;, the Refund Explanation is required.</description>
        <errorConditionFormula>AND(
OR($RecordType.DeveloperName = &apos;Credit_Card_Refund&apos;,
$RecordType.DeveloperName = &apos;NIST_Issued_Refund&apos;),
ISPICKVAL(Refund_Reason__c, &apos;Other&apos;),
ISBLANK( Refund_Explanation__c )
)</errorConditionFormula>
        <errorMessage>Refund Explanation is required if the Refund Reason is &quot;Other&quot;.</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
