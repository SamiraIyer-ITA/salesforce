    <fields>
        <fullName>Final_Report_Countdown__c</fullName>
        <description>Number of days until Final Report Due Date.</description>
        <externalId>false</externalId>
        <formula>IF(
  OR(
    ISBLANK(Scheduled_Report_Start_Date__c),
    TODAY() &lt; Scheduled_Report_Start_Date__c
  ),
  NULL,
  Final_Report_Due_Date__c - TODAY()
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Final Report Countdown</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
