    <validationRules>
        <fullName>If_ENTER_SUBJECT_is_selected_for_Subject</fullName>
        <active>true</active>
        <description>If ENTER SUBJECT from picklist is selected as the subject, display the error. User needs to write own subject.</description>
        <errorConditionFormula>(CONTAINS(Subject,&quot;ENTER SUBJECT&quot;) || (Subject= &quot;Call&quot;)) &amp;&amp; $User.ProfileId &lt;&gt; &quot;00et0000000U7uP&quot;</errorConditionFormula>
        <errorDisplayField>Subject</errorDisplayField>
        <errorMessage>Please enter a descriptive Subject.</errorMessage>
    </validationRules>
    <validationRules>
