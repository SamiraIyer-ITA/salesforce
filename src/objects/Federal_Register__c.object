    <validationRules>
        <fullName>Cannot_Edit_Locked_Record</fullName>
        <active>true</active>
        <errorConditionFormula>AND( $Permission.Edit_ADCVD_Locked_Records == false, 
NOT(ISNEW()), 
PRIORVALUE(Locked__c) == true 
)</errorConditionFormula>
        <errorMessage>This record is locked.  Please contact your administrator about unlocking this record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_reparent_a_federal_register</fullName>
        <active>false</active>
        <errorConditionFormula>AND($Setup.Master_Automation__c.ValidationRulesEnabled__c == true, Locked__c == false,AND( $Profile.Name != &apos;System Administrator&apos;,
        OR( ISCHANGED( Investigation__c ) , ISCHANGED( 
Petition__c ) , ISCHANGED( Segment__c ) ) ))</errorConditionFormula>
        <errorMessage>Cannot re-parent a federal register.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_there_is_a_parent</fullName>
        <active>true</active>
        <description>This validation rule makes sure you cannot create a federal register without a Investigation, Petition, Orders, Suspension Agreement or Segment attached to it. If all three of these lookup fields are empty, the validation error message will trigger.</description>
        <errorConditionFormula>AND($Setup.Master_Automation__c.ValidationRulesEnabled__c == true, Locked__c == false,AND( ISBLANK( Investigation__c ) , ISBLANK( Segment__c ),
ISBLANK( Petition__c ), ISBLANK(Suspension_Agreement__c ),
ISBLANK( ADCVD_Order__c )))</errorConditionFormula>
        <errorMessage>You must have a Petition, Investigation,  Orders, Suspension Agreement or Segment associated to this Federal Register.</errorMessage>
    </validationRules>
