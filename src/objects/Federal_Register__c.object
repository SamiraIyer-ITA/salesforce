CASE(Type__c,
&apos;Initiation&apos;, 0,
&apos;Postponement&apos;, 0,
&apos;Post Litigation&apos;, 0,
&apos;Correction&apos;, 0,
&apos;Termination&apos;, 0,
1)=1
)</errorConditionFormula>
        <errorDisplayField>Type__c</errorDisplayField>
        <errorMessage>The choice in the Type field is not allowed for a Federal Register entry with a Petition record.  Please update your Federal Register Type.</errorMessage>
    </validationRules>
    <validationRules>
