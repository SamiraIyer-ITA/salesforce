    <validationRules>
        <fullName>Cannot_reparent_a_federal_register</fullName>
        <active>false</active>
        <errorConditionFormula>AND( $Profile.Name != &apos;System Administrator&apos;,
        OR( ISCHANGED( Investigation__c ) , ISCHANGED( 
Petition__c ) , ISCHANGED( Segment__c ) ) )</errorConditionFormula>
        <errorMessage>Cannot re-parent a federal register.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ensure_there_is_a_parent</fullName>
        <active>true</active>
        <description>This validation rule makes sure you cannot create a federal register without a Investigation, Petition or Segment attached to it. If all three of these lookup fields are empty, the validation error message will trigger.</description>
        <errorConditionFormula>AND( ISBLANK( Investigation__c ) , ISBLANK( Segment__c ) , ISBLANK( Petition__c ))</errorConditionFormula>
        <errorMessage>You must have a Petition, Investigation, or Segment associated to this Federal Register.</errorMessage>
    </validationRules>
