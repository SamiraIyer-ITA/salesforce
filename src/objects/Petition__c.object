    <validationRules>
        <fullName>Case_number_for_AD_and_CVD_Format</fullName>
        <active>true</active>
        <description>Case Number must be formatted specifically this way.</description>
        <errorConditionFormula>IF(TEXT(Case_Type__c) = &quot;CVD&quot;, 
(NOT(OR(ISBLANK(Case_Number__c),REGEX( Case_Number__c,&quot;C-[0-9]{3}-[0-9]{3}&quot;)))), 
(NOT(OR(ISBLANK(Case_Number__c),REGEX( Case_Number__c,&quot;A-[0-9]{3}-[0-9]{3}&quot;)))) )</errorConditionFormula>
        <errorDisplayField>Case_Number__c</errorDisplayField>
        <errorMessage>Format should be A-###-### or C-###-###</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ExtensionBound</fullName>
        <active>true</active>
        <errorConditionFormula>Initiation_Extension_of_days__c &gt; 20 || Initiation_Extension_of_days__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Initiation_Extension_of_days__c</errorDisplayField>
        <errorMessage>Initiation extension cannot exceed more than 20 days.</errorMessage>
    </validationRules>
