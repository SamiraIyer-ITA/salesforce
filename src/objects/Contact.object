    <validationRules>
        <fullName>Prevent_Edit_Organizations</fullName>
        <active>true</active>
        <description>Prevent users from Changing Organization Name for an existing Contact that doesn&apos;t work at that Organization anymore, to keep that contact record with that org.

Add System admin condition eventually.
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,</description>
        <errorConditionFormula>AND(
PRIORVALUE( Sys_Prior_Account_Name__c) &lt;&gt; $Label.MCP_ExternalUserAccount,
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;Help Desk Administration&quot;,
$Profile.Name &lt;&gt; &quot;Data Team Admin&quot;,
ISCHANGED(AccountId)
)</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>You cannot change the value for Organization Name on a Contact.  Please create a new Contact record related to the new Organization.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_First_Name</fullName>
        <active>true</active>
        <description>Contact&apos;s First Name field is Required, except for Sys Admins and Data Team Profile users.

ISBLANK(FirstName)</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
$Profile.Name &lt;&gt; &quot;Data Team Admin&quot;,
$User.Id &lt;&gt; &quot;005r0000000EZ0K&quot;,
ISBLANK(FirstName)
)</errorConditionFormula>
        <errorDisplayField>FirstName</errorDisplayField>
        <errorMessage>You must enter a value for First Name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Address_Exists</fullName>
        <active>false</active>
        <description>Verify Address Fields Exist</description>
        <errorConditionFormula>ISBLANK(MailingStreet) || 
ISBLANK(MailingCity) || 
ISBLANK(MailingState) || 
ISBLANK(MailingPostalCode) || 
ISBLANK(MailingCountry) ||
ISBLANK(County__c)</errorConditionFormula>
        <errorMessage>You must enter values for Street, City, County, State, Zip &amp; Country.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Country_Exists</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
$Profile.Name &lt;&gt; &quot;System Administrator&quot;, 
$Profile.Name &lt;&gt; &quot;Data Team Admin&quot;, 
$Profile.Name &lt;&gt; &quot;Delegated Admin (Field)&quot;,
$User.Id &lt;&gt; &quot;005r0000000EZ0K&quot;,
ISBLANK(MailingCountry)
)</errorConditionFormula>
        <errorDisplayField>MailingCountry</errorDisplayField>
        <errorMessage>You must enter a value for Country.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_US_Postal_Code</fullName>
        <active>false</active>
        <description>Verify US Postal Code format

AND( 
OR(MailingCountry = &quot;USA&quot;, MailingCountry = &quot;US&quot;), 
NOT(REGEX(MailingPostalCode, &quot;\\d{5}(-\\d{4})?&quot;)) 
)</description>
        <errorConditionFormula>AND( 
OR(ISPICKVAL (MailingCountryCode, &apos;US&apos;)), 
NOT(REGEX(MailingPostalCode, &quot;\\d{5}(-\\d{4})?&quot;)) 
)</errorConditionFormula>
        <errorDisplayField>MailingPostalCode</errorDisplayField>
        <errorMessage>Zip must be in XXXXX or XXXXX-XXXX format</errorMessage>
    </validationRules>
