        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Level_3__c</fullName>
        <description>Level 3 section for this sequence.</description>
        <externalId>false</externalId>
        <inlineHelpText>Select a Level 3 section for this sequence.</inlineHelpText>
        <label>Level 3</label>
        <picklist>
            <controllingField>Level_2__c</controllingField>
            <picklistValues>
                <fullName>Article Level 3</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Mobile_Title__c</fullName>
        <description>Mobile title - Displays as the title in Mobile/Tablet View</description>
        <externalId>false</externalId>
        <inlineHelpText>This field is required if the Title is longer than 37 characters. The shorter Mobile Title makes content more readable when viewing articles on devices such as cell phones or tablets and will only appear in mobile settings.</inlineHelpText>
        <label>Mobile Title</label>
        <length>37</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Public_URL__c</fullName>
        <description>Full Public URL for Article</description>
        <externalId>false</externalId>
        <formula>HYPERLINK($Label.export_gov&amp;&quot;/article?id=&quot;&amp;UrlName, $Label.export_gov&amp;&quot;/article?id=&quot;&amp;UrlName)</formula>
        <label>Public URL</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>References__c</fullName>
        <defaultValue>&quot;Prepared by the International Trade Administration. With its network of 108 offices across the United States and in more than 75 countries, the International Trade Administration of the U.S. Department of Commerce utilizes its global presence and international marketing expertise to help U.S. companies sell their products and services worldwide. Locate the trade specialist in the U.S. nearest you by visiting http://export.gov/usoffices.&quot;</defaultValue>
        <description>Article references</description>
        <externalId>false</externalId>
        <inlineHelpText>Highlight references you want the public to know were used during content creation.</inlineHelpText>
        <label>References (Public)</label>
        <length>1000</length>
        <trackHistory>false</trackHistory>
        <type>LongTextArea</type>
        <visibleLines>3</visibleLines>
    </fields>
    <fields>
        <fullName>Series__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>Creates relationship to Series</description>
        <externalId>false</externalId>
        <inlineHelpText>Type a keyword search available Article Series.</inlineHelpText>
        <label>Series</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>Series__c.Type__c</field>
                <operation>equals</operation>
                <value>Article</value>
            </filterItems>
            <infoMessage>Type a keyword search available Article Series.</infoMessage>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Series__c</referenceTo>
        <relationshipName>Articles</relationshipName>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Stage__c</fullName>
        <description>Tracks the stages of an article through an approval process</description>
        <externalId>false</externalId>
        <inlineHelpText>Stage tracks the movement of an article through the approval process. An article may be in one of the following stages: Draft, Under Review, Submitted for Approval, Rejected, Published.</inlineHelpText>
        <label>Stage</label>
        <picklist>
            <picklistValues>
                <fullName>Draft</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Under Review</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Submitted for Approval</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Rejected</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Published</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
    <label>Guided</label>
    <pluralLabel>Guided</pluralLabel>
    <validationRules>
        <fullName>Required_Level_0</fullName>
        <active>true</active>
        <description>Level 0 is mandatory</description>
        <errorConditionFormula>ISBLANK(TEXT(Level_0__c))</errorConditionFormula>
        <errorMessage>Please pick Level 0 (Mandatory field)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Mobile_Title</fullName>
        <active>true</active>
        <description>Mobile Title Needed if Article Title is larger than 37 characters</description>
        <errorConditionFormula>AND( (LEN(Title)&gt;37) , ISBLANK( Mobile_Title__c ) )</errorConditionFormula>
        <errorMessage>Please enter the Mobile Title (Article Title is too large for Mobile view)</errorMessage>
    </validationRules>
</CustomObject>
