    <fields>
        <fullName>Level_2__c</fullName>
        <description>Level 2 sections for this sequence.</description>
        <externalId>false</externalId>
        <inlineHelpText>Select a Level 2 section for this sequence.</inlineHelpText>
        <label>Level 2</label>
        <picklist>
            <controllingField>Level_1__c</controllingField>
            <picklistValues>
                <fullName>Level 2</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
