    <validationRules>
        <fullName>Cannot_Edit_Locked_Record</fullName>
        <active>true</active>
        <errorConditionFormula>AND( $Permission.Edit_ADCVD_Locked_Records == false, 
NOT(ISNEW()),
PRIORVALUE(Locked__c) == true 
)</errorConditionFormula>
        <errorMessage>This record is locked.  Please contact your administrator about unlocking this record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>V03TollBySpecificRecordEnsureOneParent</fullName>
        <active>true</active>
        <description>You cannot create a Tolling record without a single parent record: either an Investigation, Petition, or Segment attached to it. If all three of these lookup fields are empty, the validation error message will trigger.</description>
        <errorConditionFormula>AND(
$Setup.Master_Automation__c.ValidationRulesEnabled__c = true,
Locked__c = false,
$RecordType.DeveloperName = &apos;Toll_By_Specific_Record&apos;,
(
IF(ISBLANK(Petition__c),0,1)+
IF(ISBLANK(Segment__c),0,1)+
IF(ISBLANK(Investigation__c),0,1) &lt;&gt;1
)
)</errorConditionFormula>
        <errorMessage>You must have only one of the following: Petition, Investigation, or Segment associated to this Tolling.</errorMessage>
    </validationRules>
