<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <articleTypeChannelDisplay>
        <articleTypeTemplates>
            <channel>App</channel>
            <template>Tab</template>
        </articleTypeTemplates>
        <articleTypeTemplates>
            <channel>Prm</channel>
            <template>Tab</template>
        </articleTypeTemplates>
        <articleTypeTemplates>
            <channel>Csp</channel>
            <template>Tab</template>
        </articleTypeTemplates>
        <articleTypeTemplates>
            <channel>Pkb</channel>
            <template>Toc</template>
        </articleTypeTemplates>
    </articleTypeChannelDisplay>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Input form to capture individual sections of State Report</description>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <fields>
        <fullName>Approval_Queue__c</fullName>
        <description>ITA Business Unit</description>
        <externalId>false</externalId>
        <inlineHelpText>Select the ITA business unit that is the owner of this content.</inlineHelpText>
        <label>Approval Queue</label>
        <picklist>
            <picklistValues>
                <fullName>Industry &amp; Analysis</fullName>
                <default>true</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Atom__c</fullName>
        <description>Body of an Article</description>
        <externalId>false</externalId>
        <inlineHelpText>This rich-text area is where the body of your content should be placed. The content should not deviate from the overall theme and should stand on its own as a reliable source of information. Otherwise, a new topic should be created.</inlineHelpText>
        <label>Atom</label>
        <length>32768</length>
        <trackHistory>false</trackHistory>
        <type>Html</type>
        <visibleLines>25</visibleLines>
    </fields>
    <fields>
        <fullName>Days_Since_Last_Published_Date__c</fullName>
        <externalId>false</externalId>
        <formula>TODAY() - DATEVALUE( LastPublishedDate )</formula>
        <label>Days Since Last Published Date</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Draft_Complete__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Flag the draft version as ready for approval</description>
        <externalId>false</externalId>
        <inlineHelpText>In order to submit this Atom for approval, this box must be checked.</inlineHelpText>
        <label>Draft Complete</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Internal_Comments__c</fullName>
        <description>Internal Comments</description>
        <externalId>false</externalId>
        <inlineHelpText>Add any internal comments needed.</inlineHelpText>
        <label>Internal Comments</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Level_0__c</fullName>
        <externalId>false</externalId>
        <label>Level 0</label>
        <picklist>
            <picklistValues>
                <fullName>State Report</fullName>
                <default>true</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Level_1__c</fullName>
        <description>Related Section of the State Report</description>
        <externalId>false</externalId>
        <inlineHelpText>Choose the section within your content is associated with.</inlineHelpText>
        <label>Level 1</label>
        <picklist>
            <picklistValues>
                <fullName>Exporting Companies</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Foreign Investment</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Free Trade Agreements</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Jobs Supported by Exports</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Metropolitan Exports</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>State Export Overview</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Level_2__c</fullName>
        <externalId>false</externalId>
        <label>Level 2</label>
        <picklist>
            <picklistValues>
                <fullName>N/A</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Level_3__c</fullName>
        <externalId>false</externalId>
        <label>Level 3</label>
        <picklist>
            <picklistValues>
                <fullName>NA</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Mobile_Title__c</fullName>
        <description>Title for Mobile View</description>
        <externalId>false</externalId>
        <inlineHelpText>Title for Mobile View</inlineHelpText>
        <label>Mobile Title</label>
        <length>37</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Public_URL__c</fullName>
        <description>Full Public URL for State Report Articles</description>
        <externalId>false</externalId>
        <formula>HYPERLINK($Label.export_gov&amp;&quot;/article?id=&quot;&amp;UrlName, $Label.export_gov&amp;&quot;/article?id=&quot;&amp;UrlName)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Public URL</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>References__c</fullName>
        <description>Article References</description>
        <externalId>false</externalId>
        <inlineHelpText>If there are any references that were used during the writing of the content, they can be highlighted here. The rich text editor allows for formatting and link creation.</inlineHelpText>
        <label>References (Public)</label>
        <length>1000</length>
        <trackHistory>false</trackHistory>
        <type>Html</type>
        <visibleLines>10</visibleLines>
    </fields>
    <fields>
        <fullName>Series__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <description>Lookup field to relate articles to State Reports</description>
        <externalId>false</externalId>
        <inlineHelpText>Type a keyword, such as country name and click the magnifying glass to search available Publications.</inlineHelpText>
        <label>Series</label>
        <lookupFilter>
            <active>true</active>
            <filterItems>
                <field>Series__c.Type__c</field>
                <operation>equals</operation>
                <value>State Report</value>
            </filterItems>
            <infoMessage>Search by State to locate a State Report</infoMessage>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Series__c</referenceTo>
        <relationshipLabel>State Reports Articles</relationshipLabel>
        <relationshipName>State_Reports</relationshipName>
        <required>true</required>
        <trackHistory>false</trackHistory>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Stage__c</fullName>
        <description>Tracks the stages of an Article through an approval process.</description>
        <externalId>false</externalId>
        <inlineHelpText>Stage is the phase of the process that your content is in.</inlineHelpText>
        <label>Stage</label>
        <picklist>
            <picklistValues>
                <fullName>Draft</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Under Review</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Submitted for Approval</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Rejected</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Published</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
    </fields>
    <label>State Report</label>
    <pluralLabel>State Reports</pluralLabel>
    <validationRules>
        <fullName>Required_Level_1</fullName>
        <active>true</active>
        <description>Level 1 is mandatory</description>
        <errorConditionFormula>ISBLANK(TEXT(Level_1__c))</errorConditionFormula>
        <errorMessage>Please pick Level 1 (Mandatory field)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Mobile_Title</fullName>
        <active>true</active>
        <description>Mobile Title Needed if Article Title is larger than 37 characters</description>
        <errorConditionFormula>AND( (LEN(Title)&gt;37) , ISBLANK( Mobile_Title__c ) )</errorConditionFormula>
        <errorMessage>Please enter the Mobile Title (Article Title is too large for Mobile view)</errorMessage>
    </validationRules>
</CustomObject>
