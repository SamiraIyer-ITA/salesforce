        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Modify</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/apex.js&quot;)}

var Ver = new sforce.SObject(&quot;Content_Version__c&quot;); 

var content = &quot;{!JSEncode(Content__c.Content__c)}&quot;;

Ver.Name = &quot;{!Content__c.Display_Title__c}&quot;;
Ver.Atom_Title__c = &quot;{!Content__c.Display_Title__c}&quot;;
Ver.Atom__c = &quot;{!Content__c.Id}&quot;;
Ver.Content__c = content;

Ver.Publishing_Status__c = &quot;Draft&quot;;

Ver.Version_Number__c = &apos;{!Content__c.Version_Number__c + 1}&apos;;

var result = sforce.connection.create([Ver]); 	
console.log(result);


if(result[0].success == &quot;true&quot;){ 
  window.location = &quot;/&quot; + result[0].id + &quot;/e?retURL=%2F&quot; + result[0].id; 
} else { 
  console.log(result); 
}</url>
