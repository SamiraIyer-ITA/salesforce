    <validationRules>
        <fullName>CaseType_must_match_format_of_CaseNumber</fullName>
        <active>true</active>
        <description>ADCVD Case Type should and Case Number must match</description>
        <errorConditionFormula>AND($Setup.Master_Automation__c.ValidationRulesEnabled__c == true, Locked__c == false,  
IF(
  OR(
    AND(
      OR(
         ISPICKVAL(ADCVD_Case_Type__c, &apos;AD ME&apos;),
         ISPICKVAL(ADCVD_Case_Type__c, &apos;AD NME&apos;)
         ),
         NOT(BEGINS(Name, &quot;A&quot;))
      )
      ,
     AND(
         ISPICKVAL(ADCVD_Case_Type__c, &apos;CVD&apos;),
         NOT(BEGINS(Name, &quot;C&quot;))
        )
      ),
     1, 0
    ) = 1
  )</errorConditionFormula>
        <errorDisplayField>ADCVD_Case_Type__c</errorDisplayField>
        <errorMessage>ADCVD Case Type field does not match with the ADCVD Case Number format. For AD cases the ADCVD Case Number format should be A-###-### and for CVD cases the format should be C-###-###.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_number_for_AD_and_CVD_Format</fullName>
        <active>true</active>
        <description>Case Number must be formatted specifically this way.</description>
        <errorConditionFormula>AND(
     $Setup.Master_Automation__c.ValidationRulesEnabled__c == true, 
     Locked__c == false, 
     NOT(REGEX(Name,&quot;C-[0-9]{3}-[0-9]{3}&quot;)), 
     NOT(REGEX(Name,&quot;A-[0-9]{3}-[0-9]{3}&quot;)) 
   )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>Invalid ADCVD Case Number Format. For AD cases valid Case Number format should be  A-###-###. For CVD cases valid Case Number format should be C-###-###.</errorMessage>
    </validationRules>
