        <fullName>One_Primary_Line_Item_Per_Quote</fullName>
        <active>false</active>
        <description>Only one primary line item is allowed per quote.</description>
        <errorConditionFormula>AND(Quote.PrimaryProductFamily__c != NULL,  IsPrimary__c = TRUE)</errorConditionFormula>
        <errorMessage>Only one primary line item is allowed per quote. There is an existing line item for this quote with a primary product.</errorMessage>
    </validationRules>
