        "computedFields": [
          {
            "label": "Parent_Id",
            "name": "Parent_Id",
            "saqlExpression": "case\n\nwhen 'Petition__c' is not null then 'Petition__c'\nwhen 'Investigation__c' is not null then 'Investigation__c'\nwhen 'ADCVD_Order__c' is not null then 'ADCVD_Order__c'\nwhen 'Segment__c' is not null then 'Segment__c'\nwhen 'Related_Organization__c' is not null then 'Related_Org_Parent_Id__c'\n\nend",
            "type": "Text"
          },
          {
            "defaultValue": "No Value",
            "label": "Senior_Legal",
            "name": "Senior_Legal",
            "saqlExpression": "case\nwhen 'Title__c' == \"Senior Legal\" then 'UserInfo.FirstName' + \" \" + 'UserInfo.LastName'\nend",
            "type": "Text"
          }
        ],
        "mergeWithSource": false
      },
      "sources": [
